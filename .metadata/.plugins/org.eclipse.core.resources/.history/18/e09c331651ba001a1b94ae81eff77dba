	void print(Map map)
	{
		//3 - Type code to print this map
		Set keySet = map.keySet();
		System.out.println("******************************");
		System.out.println("Key\tValue");
		Iterator it = keySet.iterator();
		while (it.hasNext())
		{
			String key = (String)it.next();
			System.out.println(key+"\t"+map.get(key));
		}
		System.out.println("******************************");
	}
